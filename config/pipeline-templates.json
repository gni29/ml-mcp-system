      }
    },
    "clustering_pipeline": {
      "name": "클러스터링 파이프라인",
      "description": "비지도 학습 클러스터링 분석",
      "category": "unsupervised_learning",
      "complexity": 0.6,
      "estimated_time": 480,
      "steps": [
        {
          "order": 1,
          "type": "data",
          "method": "load",
          "description": "데이터 로드",
          "params": {
            "numeric_columns_only": true
          },
          "outputs": ["raw_data"],
          "required": true
        },
        {
          "order": 2,
          "type": "data",
          "method": "validate",
          "description": "데이터 전처리",
          "params": {
            "handle_missing": true,
            "remove_outliers": false
          },
          "requires": ["raw_data"],
          "outputs": ["clean_data"],
          "required": true
        },
        {
          "order": 3,
          "type": "analysis",
          "method": "feature_engineering",
          "description": "특성 스케일링",
          "params": {
            "operations": ["scaling"],
            "scaling_method": "standard"
          },
          "requires": ["clean_data"],
          "outputs": ["scaled_features"],
          "required": true
        },
        {
          "order": 4,
          "type": "analysis",
          "method": "pca",
          "description": "차원 축소 (선택적)",
          "params": {
            "variance_threshold": 0.95,
            "plot_components": true
          },
          "requires": ["scaled_features"],
          "outputs": ["reduced_features", "pca_plots"],
          "required": false
        },
        {
          "order": 5,
          "type": "ml",
          "method": "clustering",
          "description": "클러스터링 수행",
          "params": {
            "algorithm": "kmeans",
            "auto_optimize": true,
            "plot_results": true
          },
          "requires": ["scaled_features"],
          "outputs": ["clusters", "cluster_plots", "metrics"],
          "required": true
        },
        {
          "order": 6,
          "type": "analysis",
          "method": "descriptive_stats",
          "description": "클러스터별 분석",
          "params": {
            "group_by_cluster": true
          },
          "requires": ["clean_data", "clusters"],
          "outputs": ["cluster_profiles"],
          "required": false
        }
      ],
      "output_summary": {
        "clusters": ["clusters"],
        "reports": ["cluster_profiles", "metrics"],
        "visualizations": ["pca_plots", "cluster_plots"],
        "data": ["scaled_features", "reduced_features"]
      }
    },
    "time_series_forecasting": {
      "name": "시계열 예측 파이프라인",
      "description": "시계열 데이터 분석 및 예측",
      "category": "time_series",
      "complexity": 0.8,
      "estimated_time": 600,
      "steps": [
        {
          "order": 1,
          "type": "data",
          "method": "load",
          "description": "시계열 데이터 로드",
          "params": {
            "parse_dates": true,
            "set_datetime_index": true
          },
          "outputs": ["time_series_data"],
          "required": true
        },
        {
          "order": 2,
          "type": "data",
          "method": "validate",
          "description": "시계열 검증",
          "params": {
            "check_frequency": true,
            "check_missing_dates": true
          },
          "requires": ["time_series_data"],
          "outputs": ["clean_ts_data"],
          "required": true
        },
        {
          "order": 3,
          "type": "analysis",
          "method": "trend_analysis",
          "description": "트렌드 분석",
          "params": {
            "method": "linear",
            "plot_trend": true
          },
          "requires": ["clean_ts_data"],
          "outputs": ["trend_analysis", "trend_plots"],
          "required": true
        },
        {
          "order": 4,
          "type": "analysis",
          "method": "seasonality",
          "description": "계절성 분석",
          "params": {
            "decomposition_method": "additive",
            "auto_detect_period": true
          },
          "requires": ["clean_ts_data"],
          "outputs": ["seasonality_analysis", "decomposition_plots"],
          "required": true
        },
        {
          "order": 5,
          "type": "analysis",
          "method": "forecasting",
          "description": "예측 모델링",
          "params": {
            "model": "arima",
            "forecast_periods": 30,
            "include_uncertainty": true
          },
          "requires": ["clean_ts_data", "trend_analysis", "seasonality_analysis"],
          "outputs": ["forecast", "forecast_plots", "model_metrics"],
          "required": true
        }
      ],
      "output_summary": {
        "forecasts": ["forecast"],
        "reports": ["trend_analysis", "seasonality_analysis", "model_metrics"],
        "visualizations": ["trend_plots", "decomposition_plots", "forecast_plots"],
        "data": ["clean_ts_data"]
      }
    }
  },
