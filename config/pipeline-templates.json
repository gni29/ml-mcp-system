{
  "common_workflows": {
    "data_exploration": {
      "name": "데이터 탐색 워크플로우",
      "description": "새로운 데이터셋에 대한 기본적인 탐색적 데이터 분석",
      "category": "exploratory",
      "complexity": 0.4,
      "estimated_time": 300,
      "steps": [
        {
          "order": 1,
          "type": "data",
          "method": "load",
          "description": "데이터 로드",
          "params": {
            "auto_detect_type": true,
            "handle_encoding": true
          },
          "outputs": ["raw_data"],
          "required": true
        },
        {
          "order": 2,
          "type": "data",
          "method": "validate",
          "description": "데이터 검증",
          "params": {
            "check_missing": true,
            "check_duplicates": true,
            "check_data_types": true
          },
          "requires": ["raw_data"],
          "outputs": ["validation_report", "cleaned_data"],
          "required": true
        },
        {
          "order": 3,
          "type": "analysis",
          "method": "descriptive_stats",
          "description": "기술통계 분석",
          "params": {
            "include_quartiles": true,
            "include_skewness": true,
            "include_kurtosis": true
          },
          "requires": ["cleaned_data"],
          "outputs": ["descriptive_stats"],
          "required": true
        },
        {
          "order": 4,
          "type": "analysis",
          "method": "correlation",
          "description": "상관관계 분석",
          "params": {
            "method": "pearson",
            "heatmap": true
          },
          "requires": ["cleaned_data"],
          "outputs": ["correlation_matrix", "correlation_heatmap"],
          "required": false
        },
        {
          "order": 5,
          "type": "visualization",
          "method": "basic_charts",
          "description": "기본 시각화",
          "params": {
            "chart_type": "auto",
            "interactive": false
          },
          "requires": ["cleaned_data", "descriptive_stats"],
          "outputs": ["basic_plots"],
          "required": false
        },
        {
          "order": 6,
          "type": "analysis",
          "method": "distribution",
          "description": "분포 분석",
          "params": {
            "plot_type": "all",
            "test_normality": true
          },
          "requires": ["cleaned_data"],
          "outputs": ["distribution_plots", "normality_tests"],
          "required": false
        }
      ],
      "output_summary": {
        "reports": ["validation_report", "descriptive_stats", "normality_tests"],
        "visualizations": ["correlation_heatmap", "basic_plots", "distribution_plots"],
        "data": ["cleaned_data", "correlation_matrix"]
      }
    },
    "data_quality_check": {
      "name": "데이터 품질 검사",
      "description": "데이터의 품질과 일관성을 확인하는 워크플로우",
      "category": "quality_assurance",
      "complexity": 0.3,
      "estimated_time": 180,
      "steps": [
        {
          "order": 1,
          "type": "data",
          "method": "load",
          "description": "데이터 로드",
          "params": {
            "validate_on_load": true
          },
          "outputs": ["raw_data"],
          "required": true
        },
        {
          "order": 2,
          "type": "data",
          "method": "validate",
          "description": "기본 검증",
          "params": {
            "comprehensive_check": true,
            "generate_report": true
          },
          "requires": ["raw_data"],
          "outputs": ["basic_validation"],
          "required": true
        },
        {
          "order": 3,
          "type": "analysis",
          "method": "outlier_detection",
          "description": "이상치 탐지",
          "params": {
            "method": "iqr",
            "visualize": true
          },
          "requires": ["raw_data"],
          "outputs": ["outlier_report", "outlier_plots"],
          "required": true
        },
        {
          "order": 4,
          "type": "analysis",
          "method": "frequency",
          "description": "빈도 분석",
          "params": {
            "include_percentages": true,
            "check_rare_values": true
          },
          "requires": ["raw_data"],
          "outputs": ["frequency_analysis"],
          "required": false
        }
      ],
      "output_summary": {
        "reports": ["basic_validation", "outlier_report", "frequency_analysis"],
        "visualizations": ["outlier_plots"],
        "quality_score": "calculated"
      }
    },
    "quick_analysis": {
      "name": "빠른 분석",
      "description": "데이터에 대한 빠른 개요 분석",
      "category": "quick",
      "complexity": 0.2,
      "estimated_time": 60,
      "steps": [
        {
          "order": 1,
          "type": "data",
          "method": "load",
          "description": "데이터 로드",
          "params": {
            "quick_mode": true
          },
          "outputs": ["data"],
          "required": true
        },
        {
          "order": 2,
          "type": "analysis",
          "method": "descriptive_stats",
          "description": "기본 통계",
          "params": {
            "quick_summary": true
          },
          "requires": ["data"],
          "outputs": ["summary_stats"],
          "required": true
        },
        {
          "order": 3,
          "type": "visualization",
          "method": "basic_charts",
          "description": "기본 차트",
          "params": {
            "chart_type": "auto",
            "quick_mode": true
          },
          "requires": ["data"],
          "outputs": ["quick_charts"],
          "required": false
        }
      ],
      "output_summary": {
        "reports": ["summary_stats"],
        "visualizations": ["quick_charts"]
      }
    }
  },
  "ml_workflows": {
    "classification_pipeline": {
      "name": "분류 모델 파이프라인",
      "description": "분류 문제를 위한 완전한 머신러닝 파이프라인",
      "category": "supervised_learning",
      "complexity": 0.8,
      "estimated_time": 900,
      "steps": [
        {
          "order": 1,
          "type": "data",
          "method": "load",
          "description": "데이터 로드",
          "params": {
            "target_column_required": true
          },
          "outputs": ["raw_data"],
          "required": true
        },
        {
          "order": 2,
          "type": "data",
          "method": "validate",
          "description": "데이터 검증",
          "params": {
            "check_target_distribution": true
          },
          "requires": ["raw_data"],
          "outputs": ["validation_report", "clean_data"],
          "required": true
        },
        {
          "order": 3,
          "type": "analysis",
          "method": "feature_engineering",
          "description": "특성 엔지니어링",
          "params": {
            "operations": ["scaling", "encoding"],
            "auto_feature_selection": true
          },
          "requires": ["clean_data"],
          "outputs": ["processed_features", "feature_info"],
          "required": true
        },
        {
          "order": 4,
          "type": "ml",
          "method": "classification",
          "description": "분류 모델 훈련",
          "params": {
            "algorithm": "random_forest",
            "cross_validation": true,
            "hyperparameter_tuning": false
          },
          "requires": ["processed_features"],
          "outputs": ["trained_model", "predictions", "metrics"],
          "required": true
        },
        {
          "order": 5,
          "type": "ml",
          "method": "evaluate",
          "description": "모델 평가",
          "params": {
            "detailed_metrics": true,
            "feature_importance": true
          },
          "requires": ["trained_model", "predictions"],
          "outputs": ["evaluation_report", "feature_importance"],
          "required": true
        },
        {
          "order": 6,
          "type": "visualization",
          "method": "ml_visualization",
          "description": "결과 시각화",
          "params": {
            "visualization_type": "confusion_matrix",
            "show_metrics": true
          },
          "requires": ["predictions", "evaluation_report"],
          "outputs": ["ml_plots"],
          "required": false
        }
      ],
      "output_summary": {
        "model": ["trained_model"],
        "reports": ["validation_report", "evaluation_report", "feature_importance"],
        "visualizations": ["ml_plots"],
        "data": ["processed_features"]
      }
    },
    "regression_pipeline": {
      "name": "회귀 모델 파이프라인",
      "description": "회귀 문제를 위한 완전한 머신러닝 파이프라인",
